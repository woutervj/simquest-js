				<computationalModel>
					<code>
						<equation>
							<variable>x_dot</variable>
							<variable>vx</variable>
						</equation>
						<equation>
							<variable>phi</variable>
							<conditional>
								<operator name="greater">
									<variable>x</variable>
									<literal>0</literal>
								</operator>
								<function name="arctan">
									<operator name="/">
										<variable>y</variable>
										<variable>x</variable>
									</operator>
								</function>
								<operator name="+">
									<literal>3.1415926535898</literal>
									<function name="arctan">
										<operator name="/">
											<variable>y</variable>
											<variable>x</variable>
										</operator>
									</function>
								</operator>
							</conditional>
						</equation>
						<equation>
							<variable>y_dot</variable>
							<variable>vy</variable>
						</equation>
						<equation>
							<variable>F</variable>
							<operator name="*">
								<operator name="-.">
									<variable>k</variable>
								</operator>
								<function name="sqrt">
									<operator name="+">
										<operator name="*">
											<variable>x</variable>
											<variable>x</variable>
										</operator>
										<operator name="*">
											<variable>y</variable>
