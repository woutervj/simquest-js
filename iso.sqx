								</function>
							</operator>
						</equation>
						<equation>
							<variable>Fy</variable>
							<operator name="*">
								<variable>F</variable>
								<function name="sin">
									<variable>phi</variable>
								</function>
							</operator>
						</equation>
						<equation>
							<variable>ay</variable>
							<operator name="*">
								<variable>m</variable>
								<variable>Fy</variable>
							</operator>
						</equation>
						<equation>
							<variable>ax</variable>
							<operator name="*">
								<variable>m</variable>
								<variable>Fx</variable>
							</operator>
						</equation>
						<equation>
							<variable>vx_dot</variable>
